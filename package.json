{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "VFA",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "node dist/main",
    "format": "prettier --write \"./src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "pre-commit": "yarn run lint-staged",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --verbose --forceExit",
    "test:e2e:file": "npm run test:e2e -t",
    "start-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.yml up --build -d"
  },
  "dependencies": {
    "@nestjs/common": "6.11.7",
    "@nestjs/core": "6.11.7",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "6.11.7",
    "@nestjs/swagger": "4.2.9",
    "@nestjs/typeorm": "^6.3.1",
    "aws-sdk": "^2.515.0",
    "axios": "^0.19.2",
    "base64-img": "^1.0.4",
    "bcrypt": "^3.0.6",
    "bestzip": "^2.1.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "csv-split-stream": "^1.0.2",
    "dotenv": "^7.0.0",
    "local": "^0.3.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql": "^2.14.1",
    "nest-router": "^1.0.9",
    "nest-schedule": "^0.6.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@nestjs/schematics": "6.9.4",
    "@nestjs/testing": "^6.5.3",
    "@types/express": "4.17.2",
    "@types/jest": "^24.0.16",
    "@types/node": "13.7.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^10.0.0-beta.14",
    "mocker-data-generator": "^2.6.6",
    "nodemon": "^1.18.11",
    "prettier": "^1.19.1",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.2",
    "xmlhttprequest": "^1.8.0",
    "@types/bcrypt": "^3.0.0",
    "@types/passport-local": "^1.0.33"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  }
}
